{
  "name": "Netbox-dev",
  "version": "2.10.4.2",
  "slug": "netbox",
  "description": "NetBox is an open source web application designed to help manage and document computer networks",
  "url": "https://github.com/casperklein/homeassistant-addons/tree/master/netbox",
  "image": "casperklein/homeassistant-netbox-dev",
  "arch": ["amd64","aarch64"],
  "startup": "application",
  "boot": "auto",
  "webui": "[PROTO:https]://[HOST]:[PORT:80]/",
  "map": ["ssl"],
  "options": {
        "user": "",
        "password": "",
	"https": false,
	"certfile": "fullchain.pem",
	"keyfile": "privatekey.pem"
  },
  "schema": {
        "user": "str",
        "password": "str",
        "https": "bool",
	"certfile": "str",
        "keyfile": "str"
  },
  "ports": {
        "80/tcp": 5580,
	"443/tcp": 5581,
	"5432/tcp": null
  },
  "ports_description": {
	  "80/tcp": "Netbox WebGUI (HTTP)",
	  "443/tcp": "Netbox WebGUI (HTTPS)",
	  "5432/tcp": "PostgreSQL"
  },
  "init": false,
  "stage": "experimental"
}
